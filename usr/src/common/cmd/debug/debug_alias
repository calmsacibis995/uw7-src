#ident	"@(#)debugger:debug_alias	1.1"
# debugger built in aliases
alias b stop
alias exit quit
alias h help
alias history fc -l
alias l list
alias s step
alias si step -i
alias next step -o
alias n step -o
alias ni step -io
alias p print
alias q quit
alias r run
alias rr fc -e -
alias sig signal
alias syms symbols
alias sys syscall
alias t stack
