alias file_make \
	create link mk_node sym_create unlink

alias file_access \
	access chg_times open_rd open_wr status sym_status

alias dir_access \
	access chg_dir chg_root chg_times status sym_status

alias dir_make \
	link mk_dir mk_mld rm_dir sym_create unlink 

