tst std_stddef note{ <stddef.h> looks ok }end compile{
	#include <sys/types.h>
	#include <stddef.h>
	ptrdiff_t i;
	size_t j;
	wchar_t k;
}end
tst std_stdlib note{ <stdlib.h> looks ok }end compile{
	#include <sys/types.h>
	#include <stddef.h>
	#include <stdlib.h>
	div_t i;
	ldiv_t j;
}end
tst std_def_calloc note{ <std*.h> defines calloc macro }end compile{
	#include <sys/types.h>
	#include <stddef.h>
	#include <stdlib.h>
	#ifndef calloc
	(
	#endif
}end
