command /bin/grep "Find Patterns in Files"

option "" "Pattern to search for" string '.*' 16
option "-v" "Print lines that don't match pattern"	boolean false
option "-l" "Print only filenames"	boolean false
option "-n" "Print line numbers"	boolean false
option "-c" "Print only a count of lines with pattern"	boolean false
option "-h" "Don't append filename to line"	boolean false

