PROGS = nuc.cdb

all::

depend::

InstallMultiple(nuc.cdb,$(USRLIBDIR)/classdb)
InstallMultiple(nwsetup.cdb,$(USRLIBDIR)/classdb)
InstallMsgCatalog(nucdt)

InstallMultiple(pixmaps/Server48.icon,$(USRLIBDIR)/pixmaps)
InstallMultiple(pixmaps/Volume48.icon,$(USRLIBDIR)/pixmaps)
InstallMultiple(pixmaps/netwareServer.icon,$(USRLIBDIR)/pixmaps)
InstallMultiple(pixmaps/netwareVolume.icon,$(USRLIBDIR)/pixmaps)
InstallMultiple(pixmaps/netwareVolumeNot.icon,$(USRLIBDIR)/pixmaps)
InstallMultiple(pixmaps/netware.xpm,$(USRLIBDIR)/pixmaps)

InstallMultiple(bitmaps/Server48.icon,$(USRLIBDIR)/bitmaps)
InstallMultiple(bitmaps/Volume48.icon,$(USRLIBDIR)/bitmaps)
InstallMultiple(bitmaps/netwareServer.icon,$(USRLIBDIR)/bitmaps)
InstallMultiple(bitmaps/netwareVolume.icon,$(USRLIBDIR)/bitmaps)
InstallMultiple(bitmaps/netwareVolumeNot.icon,$(USRLIBDIR)/bitmaps)

