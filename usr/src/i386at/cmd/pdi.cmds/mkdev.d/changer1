#ident	"@(#)pdi.cmds:mkdev.d/changer1	1.5"
#ident	"$Header$"
QUERY	-
POSTMSG	-
ALIAS	changer
DGRP	scsichanger
ATTR	scsi="true" pdimkdtab="true" bklib="SCSI" desc="SCSI Medium Changer $N$" display="true" type="changer" inquiry="$INQUIRY$"
DATA
key	minor	mode	-	/dev/mc/	-	/dev/mc/
MO	0+D*S	0666	-	cCbBtTdL	-	mcN
