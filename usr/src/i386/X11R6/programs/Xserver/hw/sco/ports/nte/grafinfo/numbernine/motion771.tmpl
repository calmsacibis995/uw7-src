VENDOR NumberNine "Number 9"
 MODEL  Motion771 "FX Motion 771"
  CLASS  N9M771 ""
   MODE   1600x1200-64K-70 "1600x1200 64K Colors, 70Hz, 4MB"

        MEMORY(@MEM_BASE@0000,0x400000);
        PORT(EFF,VGA);

        DATA
        {
        	VIDSETUP   = "/usr/X11R6.1/lib/vidconf/scripts/motion771";
                XDRIVER     = "nsf16";
                VISUAL      = "TrueColor";
                DEPTH       = 16;
                PIXWIDTH    = 1600;
                PIXHEIGHT   = 1200;
                VESABIOS6BUG = 1;
                S3HWCURSOR = 0;
                PIXTRANSEXTBUG = 1;
        }

        PROCEDURE SetGraphics
        {
                /* unlock S3 registers (relocked by int10) */
                out(0x3d4, 0x38);
                out(0x3d5, 0x48);
                out(0x3d4, 0x39);
                out(0x3d5, 0xad);

                /* specify monitor ID for desired refresh rate */
		out(0x3d4, 0x5b);
		out(0x3d5, 0x0c);
		out(0x3d4, 0x52);
		out(0x3d5, 0x02);

                /* set the graphics mode */
                r0 = 0x4F02;
                r1 = 0x8122;
                int10(r0, 2);

                /* unlock S3 registers (relocked by int10) */
                out(0x3d4, 0x38);
                out(0x3d5, 0x48);
                out(0x3d4, 0x39);
                out(0x3d5, 0xad);

                /* enable graphics engine */
                out(0x3d4, 0x40);
                in(r0, 0x3d5);
                or(r0, 0x1);
                out(0x3d5, r0);

		/* Turn on linear address mode: 8Mb */
		out(0x3d4, 0x58);
		in(r0, 0x3d5);
		or(r0, 0x13);
		out(0x3d5, r0);

        }

        PROCEDURE SetText
        {
                /* restore text mode */
                r0 = 0x0003;
                int10(r0, 1);
        }

