% 5. begin outline fonts
% Definitions to make and use outline fonts from standard PS fonts
% Including the postscript call \X'code="startoutline"'
% in your eroff source converts the current font to outline.
% Replacing startoutline with stopoutline returns to normal.

/OLstring 80 string def		% fed to makeoutlinefont
/OLtemp 10 dict def		% temporary dictionary

% The makeoutlinefont procedure creates an outline font
% from the current font.  The new font name must be placed in
% OLstring before calling the procedure.

/makeoutlinefont {
	OLtemp begin			% junk goes here
	/newname OLstring cvn def	% make OLstring a name
	/basefont currentfont def	% works on current font
	/fontsize basefont maxlength
	     1 add def			% add one for StrokeWidth
	/newid basefont /UniqueID known % does it have a UniqueID?
	{basefont /UniqueID get 1 add}	% if exists, increment
	{/fontsize fontsize 1 add def 1} % if not, make it 1
	ifelse def			% sets value of newid
	/outlinefont fontsize dict def	% sets size of outlinefont
	basefont { exch dup
	     /FID ne
	         {exch outlinefont 3 1 roll put}  % copy to outlinefont
		 {pop pop } ifelse		% if FID, do nothing

		} forall		      % for each element in basefont
	outlinefont /FontName newname put     % give it a new name
	outlinefont /FontMatrix [ 0.001 0 0 0.001 0 0] put % initial scale
	outlinefont /PaintType 2 put	      % this makes it outline font
	outlinefont /StrokeWidth 1000 36 div put  % line width=1/3 of pt size
	outlinefont /UniqueID newid put	      % give it a new id number
	newname outlinefont definefont
	end   % pop the OLdict temporary dictionary
} bind def    % end def of makeoutlinefont

% These two procedures substitute the outline font for the
% regular font, until turned off.
% They are called by startoutline, and assume OLfont contains
% the outline font dictionary


/realft /ft load bind def		% saves the real ft operator
/newft {pop OLfont realft } def		% substitutes current outline font

% The startoutline procedure assigns the outline font to OLfont.
% If it doesn't already exist, it creates it.
% Then it replaces /ft with /newft which switches the font.

/startoutline {
	restore				% cancels eroff save
%	currentpoint translate
%	.24 dup scale
%	.24 dup scale
	10 dict begin		% temporary dictionary
	/normalfont currentfont def	% saves the basefont
	/normalname normalfont /FontName get def  % gets the font name
	normalname OLstring cvs		% starts building OLstring
	0 (OL) putinterval		% replaces 1st two letters
	/OLname OLstring cvn def	% makes it a name object
	FontDirectory OLstring known	% checks to see if already exists
	{ OLname findfont }		% if exists, find it
	{ makeoutlinefont } ifelse	% otherwise, build it
	end				% dump the Startdict dictionary
	/OLfont exch def		% OLfont contains the font dictionary
	/ft /newft load def		% replaces ft
	save 				% cancels eroff restore
} def   % end definition of startoutline

% The stopoutline procedure turns off the outline font
% and restores the normal font.
% It does this by just restoring the real ft operator.

/stopoutline {
	restore			% cancels eroff save
	/ft /realft load def	% restores ft
	% currentpoint translate
	% 4.1667 dup scale
	save			% cancels eroff restore
} def	% end definition of stopoutline

% End definitions of outline fonts.
	
