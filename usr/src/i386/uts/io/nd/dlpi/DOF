SRMODE:
	PROMPT="Source Routing Mode"
	VALUES=none,auto
	DEFAULTS=auto
	TYPE=link-kit

SR_OPTIMIZATION:
	PROMPT="Route Optimization (use AREs)"
	VALUES=yes,no
	DEFAULTS=yes
	TYPE=link-kit

SOURCE_ROUTING:
	KEY=
	REQUIRED=
	ADVANCED=SRMODE,SR_OPTIMIZATION
