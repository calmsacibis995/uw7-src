#ident	"@(#)pkg.psk:psk/protos/Cbus	1.1"

!PSMDIR=/usr/src/uts/psm
!CBUSDIR=$PSMDIR/cbus

!default 775 root sys 1 NULL NULL
d none $CBUSDIR
d none $CBUSDIR/cbus.cf

!default 0444 root root 1 NULL NULL
f none $CBUSDIR/cbus.c=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus.c
f none $CBUSDIR/cbus_II.c=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus_II.c
f none $CBUSDIR/cbus_globals.c=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus_globals.c
f none $CBUSDIR/cbus_msop.c=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus_msop.c
f none $CBUSDIR/cbus.mk=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus.mk
f none $CBUSDIR/cbus.mk.dep=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus.mk.dep
f none $CBUSDIR/cbus.cf/Master=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus.cf/Master
f none $CBUSDIR/cbus.cf/System=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus.cf/System
f none $CBUSDIR/cbus.cf/Drvmap=$ROOT/usr/src/$WORK/uts/psm/cbus/cbus.cf/Drvmap
