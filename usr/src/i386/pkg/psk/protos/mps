#ident	"@(#)pkg.psk:psk/protos/mps	1.1"

!PSMDIR=/usr/src/uts/psm
!MPSDIR=$PSMDIR/mps

!default 775 root sys 1 NULL NULL
d none $MPSDIR
d none $MPSDIR/mps.cf

!default 0444 root root 1 NULL NULL
f none $MPSDIR/mps.c=$ROOT/usr/src/$WORK/uts/psm/mps/mps.c
f none $MPSDIR/mps.mk=$ROOT/usr/src/$WORK/uts/psm/mps/mps.mk
f none $MPSDIR/mps.mk.dep=$ROOT/usr/src/$WORK/uts/psm/mps/mps.mk.dep
f none $MPSDIR/mps.cf/Master=$ROOT/usr/src/$WORK/uts/psm/mps/mps.cf/Master
f none $MPSDIR/mps.cf/System=$ROOT/usr/src/$WORK/uts/psm/mps/mps.cf/System
f none $MPSDIR/mps.cf/Drvmap=$ROOT/usr/src/$WORK/uts/psm/mps/mps.cf/Drvmap
