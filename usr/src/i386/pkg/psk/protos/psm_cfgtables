#ident	"@(#)pkg.psk:psk/protos/psm_cfgtables	1.3"

!PSMDIR=/usr/src/uts/psm
!TOOLKITDIR=$PSMDIR/toolkits
!PSM_CFGTABLESDIR=$TOOLKITDIR/psm_cfgtables

!default 775 root sys 1 NULL NULL
d none $PSM_CFGTABLESDIR
d none $PSM_CFGTABLESDIR/psm_cfgtables.cf

!default 0444 root root 1 NULL NULL
f none $PSM_CFGTABLESDIR/psm_cfgtables.c=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_cfgtables/psm_cfgtables.c
f none $PSM_CFGTABLESDIR/psm_deftables.c=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_cfgtables/psm_deftables.c
f none $PSM_CFGTABLESDIR/psm_cfgtables.mk=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_cfgtables/psm_cfgtables.mk
f none $PSM_CFGTABLESDIR/psm_cfgtables.mk.dep=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_cfgtables/psm_cfgtables.mk.dep
f none $PSM_CFGTABLESDIR/psm_cfgtables.cf/Master=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_cfgtables/psm_cfgtables.cf/Master
f none $PSM_CFGTABLESDIR/psm_cfgtables.cf/System=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_cfgtables/psm_cfgtables.cf/System
