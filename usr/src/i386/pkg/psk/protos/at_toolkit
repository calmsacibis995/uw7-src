#ident	"@(#)pkg.psk:psk/protos/at_toolkit	1.1"

!PSMDIR=/usr/src/uts/psm
!TOOLKITDIR=$PSMDIR/toolkits
!AT_TOOLKITDIR=$TOOLKITDIR/at_toolkit

!default 775 root sys 1 NULL NULL
d none $AT_TOOLKITDIR
d none $AT_TOOLKITDIR/at_toolkit.cf

!default 0444 root root 1 NULL NULL
f none $AT_TOOLKITDIR/at_toolkit.c=$ROOT/usr/src/$WORK/uts/psm/toolkits/at_toolkit/at_toolkit.c
f none $AT_TOOLKITDIR/at_toolkit.mk=$ROOT/usr/src/$WORK/uts/psm/toolkits/at_toolkit/at_toolkit.mk
f none $AT_TOOLKITDIR/at_toolkit.mk.dep=$ROOT/usr/src/$WORK/uts/psm/toolkits/at_toolkit/at_toolkit.mk.dep
f none $AT_TOOLKITDIR/at_toolkit.cf/Master=$ROOT/usr/src/$WORK/uts/psm/toolkits/at_toolkit/at_toolkit.cf/Master
f none $AT_TOOLKITDIR/at_toolkit.cf/System=$ROOT/usr/src/$WORK/uts/psm/toolkits/at_toolkit/at_toolkit.cf/System
