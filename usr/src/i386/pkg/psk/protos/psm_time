#ident	"@(#)pkg.psk:psk/protos/psm_time	1.1"

!PSMDIR=/usr/src/uts/psm
!TOOLKITDIR=$PSMDIR/toolkits
!PSM_TIMEDIR=$TOOLKITDIR/psm_time

!default 775 root sys 1 NULL NULL
d none $PSM_TIMEDIR
d none $PSM_TIMEDIR/psm_time.cf

!default 0444 root root 1 NULL NULL
f none $PSM_TIMEDIR/psm_time.c=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_time/psm_time.c
f none $PSM_TIMEDIR/psm_time.mk=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_time/psm_time.mk
f none $PSM_TIMEDIR/psm_time.mk.dep=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_time/psm_time.mk.dep
f none $PSM_TIMEDIR/psm_time.cf/Master=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_time/psm_time.cf/Master
f none $PSM_TIMEDIR/psm_time.cf/System=$ROOT/usr/src/$WORK/uts/psm/toolkits/psm_time/psm_time.cf/System
