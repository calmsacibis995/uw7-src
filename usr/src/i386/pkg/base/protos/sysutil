!SRCDIR=/usr/X11R6.1
!INSTDIR=/usr/X

f sysutil $INSTDIR/lib/libhelp.so.1.0=$SRCDIR/lib/libhelp.so.1.0 0755 bin bin
f sysutil $INSTDIR/lib/libscohelp.so.1.0=$SRCDIR/lib/libscohelp.so.1.0 0755 bin bin
s sysutil /usr/lib/libhelp.so.1.0=$INSTDIR/lib/libhelp.so.1.0
s sysutil /usr/lib/libscohelp.so.1.0=$INSTDIR/lib/libscohelp.so.1.0

# Added in Gemini
f sysutil /usr/bin/bootparam 2555 bin sys 1 dev macread,dacread
f sysutil /etc/default/usemouse 0444 root sys

# Java first class execution support
f sysutil /usr/bin/javaexec 0555 bin bin

# packet filter support
f sysutil /usr/sbin/pushfilter 0555 bin bin
f sysutil /usr/lib/libfilter.so.1 0755 bin bin 1 NULL NULL
l sysutil /usr/lib/libfilter.so=/usr/lib/libfilter.so.1
d sysutil /etc/pf.d 0755 root bin
d sysutil /etc/pf.d/IP 0755 root bin
f sysutil /etc/pf.d/IP/.saps 0440 bin bin
f sysutil /etc/rc2.d/S70pf 0755 root sys

# In Gemini, ksh entries moved from cmds package to base
# package. This allows ksh-93 to be default login shell. 
# Install ksh-93 as /u95/bin/sh for UNIX95 compliance.
f sysutil /u95/bin/sh 0555 bin bin
# Make /usr/bin/ksh link to ksh-93 and install old ksh
# as ksh88.  The sequence of links to ksh-93 allows ps
# command to report "ksh" not "sh".
l sysutil /u95/bin/ksh=/u95/bin/sh
s sysutil /usr/bin/ksh=/u95/bin/ksh
f sysutil /usr/bin/ksh88=/usr/bin/ksh 555 bin bin

# Added in u213 and Gemini for Support and Escalations
f sysutil /usr/sbin/pat 555 bin bin

# Added in Gemini for lynx
f sysutil /usr/gnu/bin/lynx=/lynx/bin/lynx 0755 bin bin
s sysutil /usr/bin/lynx=/usr/gnu/bin/lynx
f sysutil /usr/gnu/etc/lynx/lynx.cfg=/lynx/etc/lynx.cfg 0644 root sys

# Added in Gemini for relax/secdefs
f sysutil /usr/sbin/relax 0755 bin bin
f sysutil /usr/sbin/secdefs 0755 bin bin
f sysutil /etc/security/seclevel/high/etc_def 0750 bin bin
f sysutil /etc/security/seclevel/high/script 0660 bin bin
f sysutil /etc/security/seclevel/improved/etc_def 0750 bin bin
f sysutil /etc/security/seclevel/improved/script 0660 bin bin
f sysutil /etc/security/seclevel/traditional/etc_def 0750 bin bin
f sysutil /etc/security/seclevel/traditional/script 0660 bin bin
f sysutil /etc/security/seclevel/low/etc_def 0750 bin bin
f sysutil /etc/security/seclevel/low/script 0660 bin bin

# Added in Gemini for Disaster Recovery (DRF)
f sysutil /usr/lib/drf/checkwhite 0544 root sys 2 NULL NULL
f sysutil /usr/lib/drf/conframdfs 0544 root sys 2 NULL NULL
f sysutil /usr/lib/drf/cpioout 0544 root sys 2 NULL NULL
f sysutil /usr/lib/drf/cut_flop 0544 root sys 2 NULL NULL
f sysutil /usr/lib/drf/disk2.files 0444 root sys 2 NULL NULL
f sysutil /usr/lib/drf/fdboot=/etc/.boot/fdboot
f sysutil /usr/lib/drf/mini_kernel 0544 root sys 2 NULL NULL
f sysutil /usr/lib/drf/mksmallfs=/etc/.boot/mksmallfs
f sysutil /usr/lib/drf/rmwhite 0544 root sys 2 NULL NULL
f sysutil /usr/lib/drf/stage2.fdinst=/etc/.boot/stage2.fdinst

# Moved from acp package, for Gemini
f sysutil /usr/bin/mapstr 511 bin bin
f sysutil /usr/bin/mapscrn 511 bin bin
f sysutil /usr/bin/mapkey 511 bin bin
f sysutil /usr/bin/mapchan 511 bin bin

# Added in Gemini
f sysutil /usr/lib/mouse/default   0644 bin bin
f sysutil /usr/lib/mouse/vi   0644 bin bin
f sysutil /usr/lib/mouse/vsh   0644 bin bin
f sysutil /usr/lib/mouse/rogue   0644 bin bin
	
#Added in Gemini for copyright display
f sysutil /usr/bin/copyrights 0555 root sys
f sysutil /etc/copyrights/copyrights.list 0444 root sys
