d sysdir /usr/lib/scohelp/en_US.ISO8859-1/man 0755 bin bin
d sysdir /usr/lib/scohelp/en_US.ISO8859-1/man/bin 0755 bin bin
s sysdir /usr/man=/usr/lib/scohelp/en_US.ISO8859-1/man

# Added in Gemini for licensing
d sysdir /pmd 0711 root root
d sysdir /var/netls 0755 bin bin
d sysdir /var/pmd 0755 bin bin
d sysdir /var/pmd/products 0755 bin bin

# added in Gemini for scohelp
d sysdir /usr/X 0755 bin bin

# added in Gemini for UNIX95
d sysdir /u95 0755 bin bin
d sysdir /u95/bin 0755 bin bin

# added in Gemini for lynx
s sysdir /etc/lynx=/usr/gnu/etc/lynx

# added in Gemini for relax/secdefs
d sysdir /etc/security/seclevel 0750 bin bin
d sysdir /etc/security/seclevel/high 0750 bin bin
d sysdir /etc/security/seclevel/improved 0750 bin bin
d sysdir /etc/security/seclevel/traditional 0750 bin bin
d sysdir /etc/security/seclevel/low 0750 bin bin

# Added for Gemini
s sysdir /ccs=/usr/ccs
s sysdir /lbin=/usr/lbin
s sysdir /share=/usr/share

