#ident	"@(#)intrfc	16.1	98/03/03"
#ident  "$Header$"

d intrfc /usr/sadm/sysadm 775 root sys 2 NULL NULL
d intrfc /usr/sadm/sysadm/bin 775 root sys 2 NULL NULL
f intrfc /usr/sadm/sysadm/bin/mkdtab 0755 bin bin 4 NULL macread,macwrite,setflevel,owner,driver,dev
f intrfc /usr/sadm/sysadm/bin/flpyconf 0755 bin bin 4 NULL macread,macwrite,setflevel,owner,driver,dev

x intrfc /usr/sadm/sysadm/add-ons 775 root sys 1 NULL NULL

d intrfc /etc/skel 0775 root sys 1 NULL NULL

f intrfc /usr/sbin/useradd 0555 root sys 2 NULL owner,audit,compat,dacread,dacwrite,filesys,macread,macwrite,multidir,setplevel,setuid,fsysrange,setflevel
f intrfc /usr/sbin/usermod 0555 root sys 2 NULL owner,audit,compat,dacread,dacwrite,filesys,macread,macwrite,multidir,setplevel,setuid,fsysrange,setflevel
f intrfc /usr/sadm/bin/usermodU 0555 root sys 1 NULL NULL
f intrfc /usr/sbin/userdel 0555 root sys 2 NULL compat,dacread,dacwrite,filesys,macread,macwrite,setflevel
f intrfc /usr/sbin/groupadd 0555 root sys 2 NULL macread,dacread,macwrite,audit,setflevel,dacwrite
f intrfc /usr/sbin/groupmod 0555 root sys 2 NULL macread,dacread,audit,dacwrite,macwrite,setflevel
f intrfc /usr/sbin/groupdel 0555 root sys 2 NULL macread,dacread,dacwrite,macwrite,setflevel

#added for gemini


f intrfc /usr/sbin/userls 0555 root sys
f intrfc /usr/sbin/groupls  0555 root sys
